{
    "version": "2.0.0",
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: g++.exe 生成活动文件",
            "command": "C:\\Program Files\\mingw64\\bin\\g++.exe", 
            "args": [
                "-mconsole",
                "-O3",
                "-mssse3", 
                "-march=native", 
                "-fdiagnostics-color=always",
                "-g",
                "${file}",
                "${workspaceFolder}/lib/uBlock_128.cpp",
                "-I", "C:/Program Files/OpenSSL-Win64/include",
                "C:/Program Files/OpenSSL-Win64/lib/VC/x64/MDd/libcrypto.lib",  
                "C:/Program Files/OpenSSL-Win64/lib/VC/x64/MDd/libssl.lib",     
                "-o","${fileDirname}\\output\\${fileBasenameNoExtension}.exe",
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": false
            },
            "detail": "调试器生成的任务。"
        }
    ]
}